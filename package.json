{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "db-push": "npx drizzle-kit push:mysql",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "generate-migration": "npx drizzle-kit generate:mysql",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "dependencies": {
    "@planetscale/database": "^1.7.0",
    "@qwikest/icons": "^0.0.8",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.25.4",
    "mysql2": "^3.3.1"
  },
  "devDependencies": {
    "@auth/core": "^0.7.1",
    "@builder.io/qwik": "^1.1.0",
    "@builder.io/qwik-auth": "~0.1.0",
    "@builder.io/qwik-city": "^1.1.0",
    "@types/eslint": "8.37.0",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "autoprefixer": "^10.4.14",
    "drizzle-kit": "^0.17.6-76e73f3",
    "eslint": "8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "^1.1.0",
    "postcss": "^8.4.23",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "^0.2.8",
    "tailwindcss": "^3.3.1",
    "typescript": "5.0.4",
    "undici": "5.22.0",
    "vercel": "^29.1.1",
    "vite": "4.3.5",
    "vite-tsconfig-paths": "4.2.0"
  }
}
